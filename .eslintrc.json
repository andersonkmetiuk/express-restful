{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "globals": {
        "$": true,
        "require": true,
        "process": true
    },
    "rules": {
        "comma-dangle": 0
    }
}
